<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
      To match TextInputLayout box stroke color for MaterialSwitch and MaterialCheckbox
      to look like TextInputLayout outlined box.

      Reference : https://stackoverflow.com/a/11019879/8146210
    -->
    <color name="outlined_box_stroke_color">#61FFFFFF</color> <!-- 38% black -->

    <color name="color_control_light">#2EFFFFFF</color> <!-- 18% white -->

    <!-- Scrim colors -->
    <color name="system_ui_scrim_light">#2EFFFFFF</color> <!-- 18% white -->
    <color name="system_ui_scrim_black">#B3000000</color> <!-- 70% black -->

    <!-- In dark mode without light status bar we can use transparent -->
    <color name="status_bar_scrim">@color/transparent</color>

    <color name="list_divider">#1fffffff</color>
</resources>